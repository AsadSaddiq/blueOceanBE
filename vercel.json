{
  "builds": [
    {
      "src": "blueOcean/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "pip": "pip"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "static" }
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "blueOcean.settings.base"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "blueOcean/wsgi.py"
    }
  ]
}
